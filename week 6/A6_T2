def main():
    print("Program starting.")
    first = input("Insert first name: ")
    last = input("Insert last name: ")
    filename = input("Insert filename: ")
    file = open(filename, "w")
    file.write(first + '\n')
    file.write(last + '\n')
    file.write('\n')
    file.close()
    print("Program ending.")
if __name__ == "__main__":
    main()

